		Scripts.Text = "Script"
		wait(0.1)
		Scripts.Text = "Scripts"
		wait(0.5)
		CloseButton.Text = " "
		CloseButton.Visible = true
		HomePg.Visible = true
		HomeMsg.Visible = true
		Credits.Visible = true
		HomeMsg:TweenSize(UDim2.new(0, 587,0, 73))
		Credits:TweenSize(UDim2.new(0, 414,0, 37))
		Close.Visible = true
		CloseButton.Text = "C"
		wait(0.1)
		CloseButton.Text = "Cl"
		wait(0.1)
		CloseButton.Text = "Clo"
		wait(0.1)
