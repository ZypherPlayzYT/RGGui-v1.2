CoolThing.Visible = false
Home.Visible = false
Homeimg.Visible = false
Scriptimg.Visible = false
GScripts.Visible = false
ComingSoon.Visible = false
Gameimg.Visible = false
ScriptPg.Visible = false
Rejoin.Visible = false
Rejoinimg.Visible = false
HomeMsg.Visible = false
Credits.Visible = false
Close.Visible = false
MainFrame.Visible = false
Scripts.Visible = false
Open.Visible = false
Frame.Visible = true


Input.MouseEnter:Connect(function()
	CoolThing.Visible = true
	CoolThing:TweenSize(UDim2.new(0.886, 0,0, 1))

end)

Input.MouseLeave:Connect(function()
	CoolThing:TweenSize(UDim2.new(0, 0,0, 1))
	wait(0.78)
	CoolThing.Visible = false

end)

CheckSystem.MouseButton1Down:Connect(function()
	if Input.Text == "AdsI8_21Ldf$!ddlsa" then
		wait()
		Frame:TweenPosition(UDim2.new(0.373, 0,0.999, 0))
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Loaded!";
			Text = "You are checked as whitelisted! Enjoy the GUI! :D";
			Button1 = "Okay";
			Duration = 8;
		})

		wait(2) 
		Title.Position = UDim2.new(0, 0,0, 0)
		ComingSoon.Size = UDim2.new(0, 0,0, 210)
		LineTop.Size = UDim2.new(0, 0,0, 6)
		Open.Position = UDim2.new(0, 0,0.988, 0)
		Title.Size = UDim2.new(0, 0,0, 36)
		MainFrame.Position = UDim2.new(0.195, 0,0.297, 0)
		MainFrame.Size = UDim2.new(0, 0,0, 346)
		Line.Size = UDim2.new(0, 3,0, 0)
		SButtons.Size = UDim2.new(0, 0,0, 310)
		HomeMsg.Size = UDim2.new(0, 587,0, 3)
		Credits.Size = UDim2.new(0, 414,0, 0)
		Title.Visible = true
		MainFrame.Visible = true
		
		
		MainFrame:TweenSize(UDim2.new(0, 760,0, 346))
		LineTop:TweenSize(UDim2.new(0, 760,0, 6))
		Title:TweenPosition(UDim2.new(0, 0,0, 0))
		MainFrame:TweenPosition(UDim2.new(0.263, 0,0.364, 0))
		Title:TweenSize(UDim2.new(0, 760,0, 36))
		wait(1)
		SButtons:TweenSize(UDim2.new(0, 120,0, 310))
		wait()
		Line:TweenSize(UDim2.new(0, 3,0, 310))
		wait(1.2)
		Home.Text = " "
		Home.Visible = true
		Homeimg.Visible = true
		Home.Text = "H"
