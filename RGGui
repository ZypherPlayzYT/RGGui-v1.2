CloseButton.MouseButton1Down:Connect(function()
	MainFrame:TweenPosition(UDim2.new(0.202, 0,0.999, 0))
	wait(1)
	MainFrame.Visible = false

	wait()
	Open.Visible = true
	Open:TweenPosition(UDim2.new(0, 0,0.786, 0))
end)

Open.MouseButton1Down:Connect(function()
	Open:TweenPosition(UDim2.new(0, 0,0.999, 0))
	wait(1)
	Open.Visible = false

	MainFrame.Visible = true
	wait()
	MainFrame:TweenPosition(UDim2.new(0.263, 0,0.364, 0))
end)

Scripts.MouseButton1Down:Connect(function()
	HomeMsg:TweenSize(UDim2.new(0, 0,0, 38))
	wait(0.2)
	HomePg.Visible = false
	ComingSoon:TweenSize(UDim2.new(0, 0,0, 210))
	wait(0.1)
	ComingSoon.Visible = false
	
	wait(1)
	ScriptPg.Size = UDim2.new(0, 0,0, 304)
	ScriptPg.Visible = true
	
	ScriptPg:TweenSize(UDim2.new(0, 637,0, 304))
	Net.Visible = true
end)

Home.MouseButton1Down:Connect(function()

	ScriptPg:TweenSize(UDim2.new(0, 0,0, 304))
	wait(0.2)
	ScriptPg.Visible = false
		
	ComingSoon:TweenSize(UDim2.new(0, 0,0, 210))
	wait(0.1)
	ComingSoon.Visible = false
	
	wait(0.1)
	HomeMsg.Visible = true
	HomeMsg:TweenSize(UDim2.new(0, 587,0, 73))
	wait(0.2)
	HomePg.Visible = true

	wait(1)

end)

